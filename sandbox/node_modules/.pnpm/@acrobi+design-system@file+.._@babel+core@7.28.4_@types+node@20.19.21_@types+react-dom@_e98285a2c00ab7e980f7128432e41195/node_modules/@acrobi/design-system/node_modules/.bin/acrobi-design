#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/@acrobi+design-system@file+.._@babel+core@7.28.4_@types+node@20.19.21_@types+react-dom@_e98285a2c00ab7e980f7128432e41195/node_modules/@acrobi/design-system/cli/node_modules:/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/@acrobi+design-system@file+.._@babel+core@7.28.4_@types+node@20.19.21_@types+react-dom@_e98285a2c00ab7e980f7128432e41195/node_modules/@acrobi/design-system/node_modules:/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/@acrobi+design-system@file+.._@babel+core@7.28.4_@types+node@20.19.21_@types+react-dom@_e98285a2c00ab7e980f7128432e41195/node_modules/@acrobi/node_modules:/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/@acrobi+design-system@file+.._@babel+core@7.28.4_@types+node@20.19.21_@types+react-dom@_e98285a2c00ab7e980f7128432e41195/node_modules:/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/@acrobi+design-system@file+.._@babel+core@7.28.4_@types+node@20.19.21_@types+react-dom@_e98285a2c00ab7e980f7128432e41195/node_modules/@acrobi/design-system/cli/node_modules:/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/@acrobi+design-system@file+.._@babel+core@7.28.4_@types+node@20.19.21_@types+react-dom@_e98285a2c00ab7e980f7128432e41195/node_modules/@acrobi/design-system/node_modules:/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/@acrobi+design-system@file+.._@babel+core@7.28.4_@types+node@20.19.21_@types+react-dom@_e98285a2c00ab7e980f7128432e41195/node_modules/@acrobi/node_modules:/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/@acrobi+design-system@file+.._@babel+core@7.28.4_@types+node@20.19.21_@types+react-dom@_e98285a2c00ab7e980f7128432e41195/node_modules:/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli/index.js" "$@"
else
  exec node  "$basedir/../../cli/index.js" "$@"
fi
