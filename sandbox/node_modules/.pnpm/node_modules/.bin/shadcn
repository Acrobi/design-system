#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/shadcn@3.4.2_@types+node@20.19.21_typescript@5.9.3/node_modules/shadcn/dist/node_modules:/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/shadcn@3.4.2_@types+node@20.19.21_typescript@5.9.3/node_modules/shadcn/node_modules:/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/shadcn@3.4.2_@types+node@20.19.21_typescript@5.9.3/node_modules:/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/shadcn@3.4.2_@types+node@20.19.21_typescript@5.9.3/node_modules/shadcn/dist/node_modules:/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/shadcn@3.4.2_@types+node@20.19.21_typescript@5.9.3/node_modules/shadcn/node_modules:/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/shadcn@3.4.2_@types+node@20.19.21_typescript@5.9.3/node_modules:/Volumes/External 2TB/bucca/REPOS/Acrobi/design-system/Acrobi-Design-System/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../shadcn/dist/index.js" "$@"
else
  exec node  "$basedir/../shadcn/dist/index.js" "$@"
fi
